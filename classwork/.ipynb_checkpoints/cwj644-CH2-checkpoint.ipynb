    "def convert(baseTenNum):\n",
    "    baseTwoNum = ''\n",
    "    while baseTenNum > 0:\n",
    "        digit = str(baseTenNum % 2)\n",
    "        baseTenNum = baseTenNum // 2\n",
    "        baseTwoNum = digit + baseTwoNum\n",
    "    return baseTwoNum\n",
    "\n",
    "n1 = convert(220)\n",
    "n2 = convert(5063)\n",
    "print(n1)\n",
    "print(n2)"
